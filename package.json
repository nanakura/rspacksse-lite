  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@egoist/tailwindcss-icons": "^1.0.7",
    "@iconify-json/carbon": "^1.1.16",
    "@rspack/cli": "0.0.25",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.2.7",
    "taze": "^0.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-rsloader": "0.0.6",
    "vue-tsc": "^1.2.0",
    "webpack": "^5.75.0"
  },
  "pnpm": {
    "ignoreMissing": [
      "react-refresh"
    ]
  },
